{
  "name": "pixi-test",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "bun check && bunx --bun vite build",
    "clean": "rm -rf .vite dist node_modules public/assets src/manifest.json",
    "clean:artifacts": "rm -rf .vite dist public/assets src/manifest.json",
    "dev": "bunx --bun vite",
    "watch": "bun ./scripts/bun/tsc-watch.ts",
    "check": "bun run typecheck && bun run lint",
    "fix": "bun run typecheck; bun run lint:fix",
    "lint": "bunx --bun eslint",
    "lint:fix": "bunx --bun eslint --fix",
    "lint:inspect": "npx @eslint/config-inspector@latest",
    "typecheck": "bun ./scripts/bun/tsc.ts",
    "typecheck:app": "bunx --bun tsc --noEmit -p ./tsconfig.app.json",
    "typecheck:vite": "bunx --bun tsc --noEmit -p ./tsconfig.node.json",
    "preview": "bunx --bun vite preview"
  },
  "dependencies": {
    "@pixi/sound": "^6.0.1",
    "@pixi/ui": "^2.2.2",
    "ky": "^1.10.0",
    "motion": "^12.4.7",
    "pixi.js": "^8.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@assetpack/core": "^1.4.0",
    "@pixi/devtools": "^2.0.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.21",
    "@types/node": "^24.3.2",
    "eslint": "^9.21.0",
    "eslint-plugin-command": "^3.3.1",
    "eslint-plugin-format": "^1.0.1",
    "pixi-stats": "^4.2.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.6",
    "vite-plugin-version-mark": "^0.2.2"
  }
}
