{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "chrome",
      "request": "launch",
      "type": "chrome",
      "internalConsoleOptions": "neverOpen",
      "url": "http://localhost:3489",
      "port": 9222,
      "webRoot": "${workspaceFolder}/current_dist/",
      "userDataDir": "${userHome}/dev/.chrome-for-vscode/Default",
      "runtimeArgs": [
        "--enable-features=DevToolsWellKnown,DevToolsAutomaticFileSystems"
      ]
    }
  ]
}
